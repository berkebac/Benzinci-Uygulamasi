<Window x:Class="WpfApplication1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApplication1"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="20*"/>
            <RowDefinition Height="299*"/>
        </Grid.RowDefinitions>
        <Menu x:Name="menu" Grid.Row="0">
            <MenuItem Header="Kayıt">
                <MenuItem Header="YakıtTürü Kayıt" Tag="YakitTürüKayıtPage" Click="mi_Click"/>
                <MenuItem Header="Pompa Kayıt" Tag="PompaKayıtPage" Click="mi_Click"/>
                <MenuItem Header="Araç Kayıt" Tag="AraçKayıtPage" Click="mi_Click"/>
                <MenuItem Header="Yakıt Alımı Kayıt" Tag="YakıtAlımıKayıtPage" Click="mi_Click"/>
            </MenuItem>
            <MenuItem Header="Yakıt Alımı" Tag="YakıtAlımıPage" Click="mi_Click"/>
            <MenuItem Header="Stok Durumu" Tag="StokDurumuPage" Click="mi_Click"/>
        </Menu>
        <Frame x:Name="frame" Padding="10" Background="SkyBlue" Grid.Row="1" Source="/WpfApplication1;component/Windows/PompaKayıtPage.xaml" Content="" NavigationUIVisibility="Hidden"/>

    </Grid>
</Window>
